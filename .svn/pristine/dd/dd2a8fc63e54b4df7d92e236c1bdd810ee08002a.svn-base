.header-contact
{
    position: relative;
    display: flex;
}
.header-contact>li
{
    margin: 0 10px;
}
.header-contact>li>a
{
    font-size: 22px;
    color:var(--light-green-color);
    -webkit-transition: all ease 0.6s;
    -moz-transition: all ease 0.6s;
    -ms-transition: all ease 0.6s;
    -o-transition: all ease 0.6s;
    transition: all ease 0.6s;

}
.header-contact>li>a:hover
