table {
	border-collapse: collapse;
	margin: 1.25em 0 0;
	width: 100%;
	border: 0;
}
table tr,
table th,
table td {
	border: 0;
}
table th,
table td {
	padding: 8px 12px;
	text-align: left;
	line-height: 26px;
}
table thead tr {
	background: #1a386e;
}
table th {
	color: #fff;
	font-size: 1em;
}
table tr td {
	background: #fff;
	color: #000000;
	font-size: 0.9em;
	border: 1px solid #ddd;
}
table tr td a {
	text-decoration: none;
}
